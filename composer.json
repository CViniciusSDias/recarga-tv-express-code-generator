{
  "name": "cviniciussdias/recargatvexpress",
  "description": "Sistema para ler e-mails recebidos em uma caixa e enviar um código para o endereço contido neles",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Vinicius Dias",
      "email": "carlosv775@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "CViniciusSDias\\RecargaTvExpress\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CViniciusSDias\\RecargaTvExpress\\Tests\\": "tests"
    }
  },
  "require": {
    "php-imap/php-imap": "^3.0",
    "symfony/mailer": "^4.3",
    "php-di/php-di": "^6.0",
    "symfony/google-mailer": "^4.3",
    "vlucas/phpdotenv": "^3.4",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-libxml": "*",
    "ext-dom": "*",
    "monolog/monolog": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "infection/infection": "*",
    "phpstan/phpstan": "^0.12.25"
  }
}
